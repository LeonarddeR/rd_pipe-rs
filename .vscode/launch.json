{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'dvc'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=dvc"
				],
				"filter": {
					"name": "dvc",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'dvc_server'",
			"cargo": {
				"args": [
					"build",
					"--bin=dvc_server",
					"--package=dvc"
				],
				"filter": {
					"name": "dvc_server",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'dvc_server'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=dvc_server",
					"--package=dvc"
				],
				"filter": {
					"name": "dvc_server",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}